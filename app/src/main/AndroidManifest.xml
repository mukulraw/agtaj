<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.annieshub.stores">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.annieshub.stores.bean"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:networkSecurityConfig="@xml/network_security_config"
        tools:targetApi="n">

        <uses-library android:name="org.apache.http.legacy" android:required="false" />

        <activity android:name="com.annieshub.stores.Splash"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.annieshub.stores.MainActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.Login"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.REgister"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.Cart"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.Wishlist"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.BillingInfo"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.ShippingInfo"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.ShippingMethod"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.PaymentInfo"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.OrderSummary"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.WebViewActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.StatusActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.Orders"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.Search"
            android:screenOrientation="portrait"/>
        <activity android:name="com.annieshub.stores.Web"
            android:screenOrientation="portrait"/>
    </application>

</manifest>